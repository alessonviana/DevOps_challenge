/bin/sh -c apt-get update &&     apt-get install ansible wget unzip python3-pip -y &&     pip3 install boto &&     wget https://releases.hashicorp.com/terraform/0.14.10/terraform_0.14.10_linux_amd64.zip -O /tmp/terraform_0.14.10_linux_amd64.zip &&     unzip /tmp/terraform_0.14.10_linux_amd64.zip -d /usr/bin